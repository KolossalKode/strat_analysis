# Polygon.io API Key (required)
# Get yours at: https://polygon.io/dashboard/api-keys
POLYGON_API_KEY=your_api_key_here

# Optional: Cache configuration
CACHE_DIR=./data/polygon_cache
CACHE_MAX_AGE_HOURS=24

# Optional: Analysis defaults
DEFAULT_MONTHS_BACK=6
MIN_HIGHER_TFS=3
DEFAULT_STOP_PCT=0.05

# Optional: Options defaults
OPTIONS_MIN_DTE=7
OPTIONS_MAX_DTE=45
OPTIONS_MIN_VOLUME=10
OPTIONS_MIN_OI=50
